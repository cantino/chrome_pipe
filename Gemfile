# A sample Gemfile
source "https://rubygems.org"

group :development do
  gem 'guard', '2.0.5'
  gem 'coffee-script'
  gem 'sass'
  gem 'yui-compressor'
  gem 'uglifier'
  gem 'webrick'

  gem 'guard-coffeescript', '1.3.4', require: false # newer wasn't working as of December 27, 2014.
  gem 'guard-sass', require: false
  gem 'guard-shell', require: false
  gem 'guard-concat', github: 'makevoid/guard-concat', require: false
end
